#include <iostream>
#include <random>

using namespace std;

int main()
{
   int n, m;
   int sum = 0;
   
   cout << "Программа находит сумму чисел ряда 1, 2, 3, 4, ... от числа n до m" << endl;
   
   cout << "Введите числа n и m: ";
   cin >> n >> m;
   
   for (int i = n; i < m; ++i) {
       cout << i << " ";
       sum += i;
   }
   
   cout << endl << "Сумма: " << sum;
   
   return 0;
}
